import com.hrappv.data.util.AbsentType;
import java.time.LocalDate;

CREATE TABLE IF NOT EXISTS Absense (
    absnt_type   TEXT ,--AS  AbsentType NOT NULL,
    absnt_date   TEXT ,--AS LocalDate NOT NULL,
