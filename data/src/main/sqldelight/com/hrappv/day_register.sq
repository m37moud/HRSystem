CREATE TABLE IF NOT EXISTS day_register (
	emp_name	INTEGER,
	department	INTEGER,
	ODATE	TEXT,
	day	TEXT,
	status	TEXT,
	in_time	TEXT,
	out_time	TEXT,
	late	REAL AS Float,
	early	REAL AS Float
);



